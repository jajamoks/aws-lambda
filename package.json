{
  "name": "lambda-service",
  "version": "1.0.0",
  "description": "aws lambda service boilerplate",
  "scripts": {
    "build": "npm run clean && webpack --mode=production",
    "check-types": "tsc",
    "dev": "cross-env LOG_LEVEL=info nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --require dotenv-flow/config --exec ts-node -r tsconfig-paths/register src/index.local.ts",
    "test:coverage": "cross-env NODE_ENV=test nyc --reporter=text-summary --reporter=text --reporter=lcov mocha --reporter mocha-sonarqube-reporter --reporter-options output=test-report.xml --timeout 3000 --recursive -r ts-node/register -r tsconfig-paths/register 'src/**/*.spec.ts'",
    "test": "cross-env NODE_ENV=test mocha --timeout 3000 --recursive -r ts-node/register -r tsconfig-paths/register 'src/**/*.spec.ts'",
    "debug": "node --inspect-brk nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node src/index.local.ts --require dotenv-flow/config -r",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier": "prettier --check 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "start": "node ./dist/index.js",
    "clean": "rimraf dist/"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts",
      "src/**/*.local.ts",
      "src/utils/logger.ts",
      "src/fixtures/**",
      "src/interfaces/**",
      "src/types/**"
    ],
    "all": true
  },
  "engines": {
    "node": ">=12.20.1"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.14.28",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "aws-lambda": "^1.0.6",
    "aws-sdk-mock": "^5.2.1",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-filename-rules": "^1.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.2",
    "mocha": "^9.1.1",
    "mocha-sonarqube-reporter": "^1.0.2",
    "nodemon": "^2.0.12",
    "nyc": "^15.1.0",
    "prettier": "^2.4.0",
    "prettier-eslint": "^13.0.0",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.1.5",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "aws-sdk": "^2.939.0",
    "axios": "^0.21.1",
    "babel-runtime": "^6.26.0",
    "core-js": "^3.17.3",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "url-record": "^1.0.3",
    "winston": "^3.3.3"
  }
}
